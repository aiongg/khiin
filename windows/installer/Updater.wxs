<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">

    <?include Defines.wxi ?>

    <Fragment>
        <CustomAction Id="UpdateDatabaseCA"
                      Directory="INSTALLDIR64"
                      ExeCommand="&quot;[INSTALLDIR64]$(var.KhiinSettingsExeTrg)&quot; -old &quot;[APPDATADIR]$(var.KhiinDatabaseTrg)&quot; -new &quot;[INSTALLDIR64]$(var.KhiinDatabaseTrg)&quot;"
                      Execute="deferred"
                      Return="check"
                      Impersonate="no"
                      />

        <InstallExecuteSequence>
            <Custom Action="UpdateDatabaseCA" After="InstallFiles">NOT Installed</Custom>
        </InstallExecuteSequence>
    </Fragment>
</Wix>
